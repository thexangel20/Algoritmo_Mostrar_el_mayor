Algoritmo Mostrar_el_mayor
	Escribir 'Ingrese el número1'
	Leer número1
	Escribir 'Ingrese el número2'
	Leer número2
	Si número1==número2 Entonces
		Escribir 'Preposición no valida'
	SiNo
		Si número1 > número2 Entonces
			Escribir 'El número mayor es:'
			Escribir número1
		SiNo
			Escribir 'El número mayor es:'
			Escribir número2
		FinSi
	FinSi
FinAlgoritmo
